@font-face {
  font-family: "yaaak";
  src: url(<%= asset_path('yaaak.eot') %>);
  src: url(<%= asset_path('yaaak.eot') + '?#iefix' %>) format("embedded-opentype"),
    url(<%= asset_path('yaaak.ttf') %>) format("truetype"),
    url(<%= asset_path('yaaak.svg') + '#yaaak' %>) format("svg"),
    url(<%= asset_path('yaaak.woff') %>) format("woff");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "yaaak" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  @include font-size(14);
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="i-"]:before,
[class*=" i-"]:before {
  font-family: "yaaak" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  @include font-size(14);
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}



